{
  "name": "server",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "seed": "bun run seed/index.ts",
    "start": "bun run dist/index.mjs"
  },
  "dependencies": {
    "@Hackron/db": "workspace:*",
    "@Hackron/env": "workspace:*",
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^1.1.18",
    "@langchain/langgraph": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "catalog:",
    "express": "^5.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@Hackron/config": "workspace:*",
    "@types/bun": "catalog:",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "tsdown": "^0.16.5",
    "typescript": "catalog:"
  }
}